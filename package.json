{
  "name": "finagram-react",
  "author": "Alexey Stratan",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://lehos.github.io/finagram-react",
  "scripts": {
    "dev": "webpack-dev-server --hot",
    "build": "yarn clean && webpack --mode=production",
    "clean": "rimraf build",
    "format": "cross-env prettier --write './src/**/*.{ts,tsx}'",
    "lint": "yarn lint:js",
    "lint:js": "eslint './src/**/*.{ts,tsx}'",
    "ts": "tsc",
    "bundle-analyzer": "cross-env NODE_ENV=production BUNDLE_ANALYZER=localhost webpack",
    "upgrade-interactive": "npm-check --update",
    "predeploy": "cross-env HISTORY_HASH=true ASSET_PATH=/finagram-react/ yarn build",
    "deploy": "gh-pages -d build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn ts"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "3.20.2",
    "fast-memoize": "^2.5.1",
    "final-form": "4.17.0",
    "history": "4.9.0",
    "linaria": "1.3.1",
    "nanoid": "2.0.3",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-easy-state": "6.1.3",
    "react-final-form": "6.3.0",
    "react-hot-loader": "4.12.6",
    "react-router-dom": "5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.5.0",
    "@babel/preset-env": "7.5.4",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@hot-loader/react-dom": "^16.8.6",
    "@svgr/webpack": "4.3.1",
    "@types/nanoid": "2.0.0",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.4",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@typescript-eslint/parser": "1.12.0",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "1.12.0",
    "cross-env": "5.2.0",
    "css-loader": "3.0.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "file-loader": "4.0.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.0",
    "lint-staged": "9.2.0",
    "mini-css-extract-plugin": "0.7.0",
    "npm-check": "5.9.0",
    "prettier": "1.18.2",
    "prop-types": "15.7.2",
    "rimraf": "2.6.3",
    "style-loader": "0.23.1",
    "stylis": "3.5.4",
    "typescript": "3.5.3",
    "webpack": "4.35.3",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2"
  }
}
